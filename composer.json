{
    "name": "partridge-rocks/larahub",
    "description": "A package aimed at bridging the gap between Laravel and GitHub",
    "keywords": [
        "partridge-rocks",
        "larahub",
        "laravel",
        "github",
        "php"
    ],
    "homepage": "https://github.com/partridge-rocks/larahub",
    "license": "MIT",
    "authors": [
        {
            "name": "Jordan Partridge",
            "email": "jordan@partridge.rocks",
            "homepage": "https://partridge.rocks",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^3.3",
        "tightenco/duster": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "PartridgeRocks\\Larahub\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PartridgeRocks\\Larahub\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PartridgeRocks\\Larahub\\LarahubServiceProvider"
            ],
            "aliases": {
                "Larahub": "PartridgeRocks\\Larahub\\LarahubFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
